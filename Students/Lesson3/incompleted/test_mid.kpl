PROGRAM NTT;
TYPE T = INTEGER;
VAR N : INTEGER;
    A : ARRAY(. 10 .) OF T;

PROCEDURE INPUT;
VAR I : INTEGER;
BEGIN
    FOR I:=1 TO N DO
        A(.I.) := READI;
END;

PROCEDURE OUTPUT;
VAR I : INTEGER;
BEGIN
    FOR I:= N TO 1 DO
        BEGIN   
            CALL WRITEI(A(.I.));
            CALL WRITELN;
        END
END;

FUNCTION SUM: INTEGER;
VAR I : INTEGER;
    S : INTEGER;
BEGIN
    S := 0;
    FOR I := 1 TO N DO  
        S := S + A(.I.);
    SUM := S;
END;

BEGIN
    N := READI;
    CALL INPUT;
    CALL OUTPUT;
    CALL WRITEI(SUM);
END.

